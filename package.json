{
  "name": "server-boilerplate-djs",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "nyc mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "NODE_ENV=test npm run migrate",
    "migrate:test:cesar": "set NODE_ENV=test&&npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "12.16.3"
  },
  "dependencies": {
    "@tensorflow-models/toxicity": "^1.2.2",
    "@tensorflow/tfjs": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "mocha": "^7.1.2",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pg": "7.8.0",
    "postgrator-cli": "3.1.0",
    "supertest": "^4.0.2",
    "winston": "^3.2.1",
    "xss": "^1.0.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1"
  }
}
